      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon1.png",
        "backgroundColor": "#332424"
      }
    },
    "web": {
      "favicon": "./assets/logo1.png"
    }
  }
}
